{
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "globals": {
        "require": true,
        "module": true
    },
    "plugins": [],
    "rules": {
        "curly": 2,
        "eqeqeq": 2,
        "comma-dangle": 2,
        "quotes": [2, "single"],
        "no-use-before-define": 0,
        "no-underscore-dangle": 0,
        "strict": 0,
        "no-alert": 0,
        "no-empty": 0,
        "consistent-return": 0,
        "camelcase": 0,
        "max-len": [2, 120, 4]
    }
}
